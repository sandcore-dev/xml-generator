{
  "name": "sandcore-dev/xml-generator",
  "description": "Generates fake XML data with Faker based on output from the XML analyzer",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sandcore Development",
      "email": "info@sandcore.dev"
    }
  ],
  "require": {
    "php": "^7.3",
    "fakerphp/faker": "^1.13",
    "sandcore-dev/xml-analyzer": "^0.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SandcoreDev\\XmlGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SandcoreDev\\XmlGenerator\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
